
 # Copy this file to `.env.local` (same folder) before deploying.
 # Do NOT commit secrets.
 
 # Deployer wallet (EOA) used to deploy contracts
 DEPLOYER_PRIVATE_KEY=
 
 # Paymaster signing key used by deploy script to derive PAYMASTER_VERIFYING_SIGNER
 # You can use a single key for all chains by setting this.
 PAYMASTER_SIGNING_KEY=
 
 # Optional: per-chain override (uncomment if you want distinct keys)
 # PAYMASTER_SIGNING_KEY_8453=
 # PAYMASTER_SIGNING_KEY_1=
 # PAYMASTER_SIGNING_KEY_137=
 # PAYMASTER_SIGNING_KEY_42161=
 # PAYMASTER_SIGNING_KEY_10=
 # PAYMASTER_SIGNING_KEY_56=
 # PAYMASTER_SIGNING_KEY_43114=
 # PAYMASTER_SIGNING_KEY_59144=
 
 # Universal v2 deterministic deployment
 UNIVERSAL_CREATE2_SALT=xvault-universal-v2-2026-01-07
 DETERMINISTIC_DEPLOY=1
 FORCE_FACTORY_REDEPLOY=1
 
 # RPC URLs (recommended: use a reliable provider)
 BASE_RPC_URL=https://mainnet.base.org
 ETH_RPC_URL=https://ethereum-rpc.publicnode.com
 POLYGON_RPC_URL=https://polygon-bor.publicnode.com
 ARBITRUM_RPC_URL=https://arbitrum-one-rpc.publicnode.com
 OPTIMISM_RPC_URL=https://optimism-rpc.publicnode.com
 BSC_RPC_URL=https://bsc-dataseed.binance.org
 AVALANCHE_RPC_URL=https://api.avax.network/ext/bc/C/rpc
 LINEA_RPC_URL=https://rpc.linea.build
 
 # Gas / fee overrides (optional)
 # Polygon often requires higher priority fee.
 DETERMINISTIC_GAS_LIMIT_137=2500000
 DEPLOY_MAX_FEE_GWEI_137=80
 DEPLOY_TIP_GWEI_137=30
 
 # Optional: override EntryPoint per chain if needed (defaults to 0x5FF1...)
 # ENTRY_POINT_ADDRESS_8453=
 # ENTRY_POINT_ADDRESS_1=
 # ENTRY_POINT_ADDRESS_137=
 # ENTRY_POINT_ADDRESS_42161=
 # ENTRY_POINT_ADDRESS_10=
 
 # --- Passkey/WebAuthn on-chain defaults for new accounts (XAccount implementation) ---
 # RP ID that your frontend/backend uses for WebAuthn (e.g. zaur.at). Used to derive rpIdHash = sha256(RP_ID).
 RP_ID=zaur.at
 # Optional: provide the rpIdHash directly (takes precedence over RP_ID).
 # RP_ID_HASH=
 
 # Require User Verification (UV) flag in authenticatorData.
 # Defaults to true. Set to 0 or false to disable.
 REQUIRE_UV=1
